<!DOCTYPE HTML>
<html lang="fr">
<head>
    <title>LoL Flex S10 | {% block title %}{% endblock %}</title>
    <meta charset="utf-8" />
    <link rel="stylesheet" type="text/css" href="{{ asset("vendors/fontawesome-free/css/all.min.css") }}" />
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" />
    <link rel="stylesheet" type="text/css" href="{{ asset("css/sb-admin-2.min.css") }}" />
    {% block css %}{% endblock %}
    <link rel="favicon icon" type="image/icon" href="{{ asset("favicon.ico") }}" />
</head>
<body id="page-top">
<div id="wrapper">
    {% include "menu.html.twig" %}
    <div id="content-wrapper" class="d-flex flex-column">
        <div id="content">
            <div class="container-fluid">
                {% block body %}{% endblock %}
            </div>
        </div>
        {% include "footer.html.twig" %}
    </div>
</div>
<script src="{{ asset("vendors/jquery/jquery.min.js") }}"></script>
<script src="{{ asset("vendors/bootstrap/js/bootstrap.bundle.min.js") }}"></script>
<script src="{{ asset("vendors/jquery-easing/jquery.easing.min.js") }}"></script>
<script src="{{ asset("js/sb-admin-2.min.js") }}"></script>
<script src="{{ asset("vendors/chart.js/Chart.min.js") }}"></script>
{% block js %}{% endblock %}
</body>
</html>